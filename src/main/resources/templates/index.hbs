{{#partial "content"}}
    <div class="entry-list">

        {{#each entries}}
            <article>
                <h2><a href="detail.html">{{title}}</a></h2>
                <time datetime="{{entryDateTime}}">{{entryDateTime}}</time>
            </article>
        {{else}}
            <article>
                <h2>No content</h2>
            </article>
        {{/each}}
    </div>
{{/partial}}

{{> base.hbs}}